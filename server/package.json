{
  "name": "deltaport",
  "version": "1.0.0",
  "private": true,
  "description": "Crypto trading app",
  "email": "test@test.com",
  "homepage": "./",
  "license": "ISC",
  "main": "electron.js",
  "author": {
    "name": "J P",
    "email": "jp@gmail.com",
    "url": "https://www.google.com"
  },
  "scripts": {
    "start": "node dist/index.js",
    "start-watch": "nodemon",
    "start-ts": "ts-node src/index.ts",
    "exchange-test": "ts-node testExchange.ts",
    "test": "jest",
    "test-queue": "jest -t 'test-queue'",
    "build": "npm run build-ts",
    "watch": "npm run watch-ts",
    "watch-test": "npm run test --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "build-electron": "electron-builder -mwl",
    "start-electron": "electron ."
  },
  "build": {
    "appId": "com.deltaport.app",
    "productName": "Deltaport",
    "npmRebuild": "true",
    "asar": false,
    "files": [
      "dist",
      "node_modules",
      "electron.js",
      "package.json",
      "assets/icon.png"
    ],
    "extraResources": [
      {
        "from": "../client/build",
        "to": "client/build"
      }
    ],
    "directories": {
      "buildResources": "assets",
      "output": "electron-dist"
    },
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "deb"
    },
    "artifactName": "Deltaport_${version}_Setup.${ext}",
    "forceCodeSigning": false
  },
  "dependencies": {
    "@tensorflow/tfjs": "^3.12.0",
    "@truffle/hdwallet-provider": "^2.0.0",
    "appdirectory": "^0.1.0",
    "bignumber.js": "^9.0.2",
    "bull": "^4.2.0",
    "ccxt": "^1.66.20",
    "compression": "^1.7.4",
    "debug": "^4.3.3",
    "devalpha": "^0.3.5",
    "dotenv": "^10.0.0",
    "ethers": "^5.5.4",
    "express": "^4.17.2",
    "express-validator": "^6.14.0",
    "express-winston": "^4.2.0",
    "graphql": "^16.2.0",
    "graphql-request": "^3.7.0",
    "helmet": "^5.0.0",
    "ioredis": "^4.28.2",
    "is-electron": "^2.2.1",
    "mysql2": "^2.3.2",
    "sequelize": "^6.12.4",
    "short-uuid": "^4.2.0",
    "socket.io": "^4.4.0",
    "socket.io-emitter": "^3.2.0",
    "socket.io-redis": "^6.1.1",
    "sqlite3": "^5.0.2",
    "superagent": "^6.1.0",
    "tulind": "^0.8.20",
    "umzug": "^3.0.0",
    "vm2": "^3.9.5",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5",
    "winston-elasticsearch": "^0.16.0"
  },
  "devDependencies": {
    "@types/bull": "^3.15.7",
    "@types/compression": "^1.7.2",
    "@types/debug": "^4.1.7",
    "@types/ethereum-protocol": "^1.0.2",
    "@types/express": "^4.17.13",
    "@types/express-winston": "^4.0.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.6",
    "@types/socket.io": "^3.0.2",
    "@types/socket.io-redis": "^3.0.0",
    "asar": "^3.1.0",
    "copyfiles": "^2.4.1",
    "electron": "^16.0.5",
    "electron-builder": "^22.14.12",
    "jest": "^27.4.5",
    "nodemon": "^2.0.14",
    "sequelize-cli": "^6.3.0",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "typescript": "^4.5.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}
